<div
  class="hero min-h-screen"
  style="background-image: url(#);"
>
  <section class="flex items-stretch">

    <div class="card flex-shrink-0 w-full max-w-sm shadow-2xl bg-info m-8">
      <div class="card-body">
        <div class="form-control">
          <label class="input-group">
            <input
              id="player"
              type="text"
              placeholder="playername"
              class="input input-bordered"
            />
          </label>
        </div>
        <div class="form-control">
          <label class="input-group">
            <input
              id="pass"
              type="password"
              placeholder="password"
              class="input input-bordered"
            />
          </label>
        </div>
        <div class="form-control">
          <label class="input-group">
            <input
              id="qmName"
              type="text"
              placeholder="Questmaster Name"
              class="input input-bordered"
            />
          </label>
        </div>
        <div class="card-actions justify-start">
          <button id="submit" class="btn">SIGN UP</button>
        </div>
      </div>
    </div>
  </section>
</div>
